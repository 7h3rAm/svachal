querying and analysis for writeup ymls
  machinescli --query oscplike
  machinescli --query htb
  machinescli --info htb#208
  machinescli --info htb#208 -j
  machinescli --info htb#208 -j | jq '.[]|.points'
  machinescli --info htb#208 -j | jq '.[]|.points == 30'
  machinescli --query '.machines[]|.points == 30'
  machinescli --query '.machines[]|select(.points == 10)'
  machinescli --query '.machines[]|select(.points == 20)'
  machinescli --query '.machines[]|select(.points == 30)'
  machinescli --query '.machines[]|select(.points == 40)'
  machinescli --query '.machines[]|select(.points == 50)'
  machinescli --query '.machines[]|select(.points == 50 and .os == "Windows")'
  machinescli --query '.machines[]|select(.points == 50 and .os != "Windows")'
  machinescli --query '.machines[]|select(.points == 20 and .os != "Windows")'

